# Sentinel Diagnostics - Environment Variables
# Team Victoriors - AI Medical Platform Configuration
# DO NOT COMMIT THIS FILE TO VERSION CONTROL

# ==============================================================================
# API KEYS - SECURITY CRITICAL
# ==============================================================================

# Roboflow API for Brain Tumor Detection
ROBOFLOW_API_KEY=your_roboflow_api_key_here
ROBOFLOW_API_URL=https://serverless.roboflow.com/
ROBOFLOW_BRAIN_MODEL_ID=brain-tumor-segmentation-dwapu-xxwhv/3

# Google Gemini AI API (for AI Assistant Sage)
GEMINI_API_KEY=your-gemini-api-key-here

# ==============================================================================
# FLASK APPLICATION SETTINGS
# ==============================================================================

# Flask Secret Key for Sessions
SECRET_KEY=sentinel-diagnostics-team-victoriors-secret-key-2025

# Flask Environment
FLASK_ENV=development
FLASK_DEBUG=True

# Application Host and Port
FLASK_HOST=0.0.0.0
FLASK_PORT=5000

# ==============================================================================
# AI MODEL CONFIGURATION
# ==============================================================================

# AI Processing Settings
AI_DEVICE=auto
AI_BATCH_SIZE=1
AI_CONFIDENCE_THRESHOLD=0.25

# ==============================================================================
# SECURITY SETTINGS
# ==============================================================================

# Password requirements
MIN_PASSWORD_LENGTH=8
REQUIRE_SPECIAL_CHARS=True
REQUIRE_NUMBERS=True
REQUIRE_UPPERCASE=True

# Session timeout (in minutes)
SESSION_TIMEOUT_MINUTES=60

# ==============================================================================
# MEDICAL PLATFORM SETTINGS
# ==============================================================================

# Platform Information
PLATFORM_NAME=Sentinel Diagnostics
PLATFORM_VERSION=1.0
ORGANIZATION_NAME=Team Victoriors

# Medical Compliance
HIPAA_COMPLIANT=True
AUDIT_LOGGING=True

# ==============================================================================
# FEATURE FLAGS
# ==============================================================================

# Enable/Disable Platform Features
ENABLE_AI_DIAGNOSTICS=True
ENABLE_VIDEO_CONSULTATIONS=True
ENABLE_PAYMENTS=True
ENABLE_MOBILE_APP=True
ENABLE_MULTI_LANGUAGE=True

# AI Model Feature Flags
ENABLE_BRAIN_TUMOR_DETECTION=True
ENABLE_LUNG_CANCER_DETECTION=True
ENABLE_BREAST_CANCER_DETECTION=True
ENABLE_AI_ASSISTANT=True